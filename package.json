{
  "name": "poweradmin",
  "version": "1.0.0",
  "description": "[Poweradmin](https://www.poweradmin.org) is a friendly web-based DNS administration tool for PowerDNS server.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:debug": "cypress open --e2e --browser chrome",
    "test:e2e:chromium": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:webkit": "cypress run --browser webkit",
    "test:e2e:report": "cypress run --reporter spec"
  },
