#, php-format
msgid "There is no matching reverse-zone for: %s."
msgstr "No hay zona reversa coincidente para: %s."

#: lib/Domain/Service/ReverseRecordCreator.php:70
#, php-format
msgid "A PTR record for %s pointing to %s already exists."
msgstr "Ya existe un registro PTR para %s apuntando a %s."

#: lib/Domain/Service/Dns/RecordManager.php:103
msgid "You do not have the permission to add SOA record."
msgstr "No tiene permiso para agregar registro SOA."

#: lib/Domain/Service/Dns/RecordManager.php:107
msgid "You do not have the permission to add NS record."
